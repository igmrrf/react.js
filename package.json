{
  "name": "react-grmfs",
  "private": true,
  "version": "0.0.1",
  "type": "module",
  "author": {
    "name": "igmrrf",
    "email": "hello@igmrrf.com",
    "url": "https://github.com/igmrrf"
  },
  "keywords": [
    "react",
    "redux",
    "redux-toolkit",
    "firebase",
    "firestore",
    "redux-toolkit-query",
    "socket.io",
    "graphql",
    "apollo",
    "material-ui",
    "funckage",
    "sentry",
    "typescript",
    "cypress",
    "vite",
    "jest",
    "eslint",
    "roboto",
    "rtk-query",
    "normalizr"
  ],
  "license": "MIT",
  "description": "A simple react app with redux state management using typicode's jsonplaceholder fake api and other free and open source packages",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "lint": "eslint src --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "test": "jest --watchAll --verbose --coverage",
    "e2e": "cypress open",
    "preview": "vite preview",
    "schema:download": "npx apollo service:download --endpoint:https://api.hashnode.com ./src/app/graphql/graphql-schema.json",
    "schema:generate-watch": "npx apollo codegen:generate --localSchemaFile=./src/app/graphql/graphql-schema.json --target=typescript --tagName=gql --watch"
  },
  "dependencies": {
    "@alwaysmeticulous/recorder-loader": "^2.53.0",
    "@apollo/client": "^3.7.14",
    "@emotion/react": "^11.11.0",
    "@emotion/styled": "^11.11.0",
    "@fontsource/roboto": "^5.0.0",
    "@mui/icons-material": "^5.11.16",
    "@mui/material": "^5.13.1",
    "@reduxjs/toolkit": "^1.9.5",
    "@rtk-query/graphql-request-base-query": "^2.2.0",
    "@sentry/react": "^7.53.0",
    "axios": "^1.4.0",
    "checkout-print": "1.0.6",
    "firebase": "^9.22.0",
    "funckage": "^0.1.6",
    "graphql": "^16.6.0",
    "graphql-request": "^6.1.0",
    "normalizr": "^3.6.2",
    "notistack": "^3.0.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-error-boundary": "^4.0.4",
    "react-gtm-module": "^2.0.11",
    "react-redux": "^8.0.5",
    "react-redux-firebase": "^3.11.0",
    "react-router-dom": "^6.11.2",
    "redux-firestore": "^2.0.1",
    "redux-persist": "^6.0.0",
    "socket.io-client": "^4.6.1"
  },
  "devDependencies": {
    "@types/jest": "^29.5.1",
    "@types/node": "^20.2.3",
    "@types/react": "^18.0.28",
    "@types/react-dom": "^18.0.11",
    "@types/react-gtm-module": "^2.0.1",
    "@typescript-eslint/eslint-plugin": "^5.57.1",
    "@typescript-eslint/parser": "^5.57.1",
    "@vitejs/plugin-react": "^4.0.0",
    "cypress": "^12.12.0",
    "eslint": "^8.38.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.3.4",
    "jest": "^29.5.0",
    "typescript": "^5.0.2",
    "vite": "^4.3.2"
  }
